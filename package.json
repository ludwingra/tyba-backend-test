{
    "name": "tyba-backend-engineer-test",
    "version": "1.0.0",
    "description": "Tyba Backend Engineer test",
    "scripts": {
        "dev": "export ENV=dev && nodemon index.ts",
        "start:local": "npm run build-ts && export ENV=local && cp -r environments/. dist/environments && node dist/index.js",
        "build-ts": "rm -rf dist && tsc --sourceMap false",
        "copy_env_dev": "mkdir -p ./dist/environments && cp environments/.dev.env dist/environments/.",
        "build:dev": "npm run build-ts && npm run copy_env_dev && cp package.json ./dist/ && cp Dockerfile.dev ./dist/Dockerfile",
        "start_serve_dev": "npm i --production && export ENV=dev && node index.js",
        "start_serve": "npm i --production && export ENV=prod && node index.js"
    },
    "dependencies": {
        "@hapi/boom": "^9.1.3",
        "axios": "^0.27.2",
        "bcrypt": "^5.0.1",
        "chalk": "^4.1.1",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-validator": "^6.12.0",
        "form-data": "^4.0.0",
        "jsonwebtoken": "^8.5.1",
        "mysql2": "^2.2.5",
        "sequelize": "^6.6.5",
        "underscore": "^1.13.3",
        "sharp": "^0.30.7",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/compression": "^1.7.1",
        "@types/cors": "^2.8.12",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/express-validator": "^3.0.0",
        "@types/jsonwebtoken": "^8.5.6",
        "@types/mysql": "^2.15.19",
        "@types/underscore": "^1.11.4",
        "@types/sharp": "^0.30.4",
        "@types/uuid": "^8.3.4",
        "nodemon": "^2.0.12",
        "ts-node": "^10.1.0",
        "typescript": "^4.3.5"
    },
    "author": "Ludwing Rivera Amador",
    "license": "ISC"
}
